{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "NVs.OccupancySensor.API.MQTT.HomeAssistantMqttAdapter": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Version": "0.0.0",
  "CV": {
    "Capture": {
      "Source": "https://anthonygiretti.blob.core.windows.net/videos/earth.mp4",
      "FrameInterval": "00:00:00.3000000"
    },
    "Detection": {
      "Threshold": "0.1"
    }
  },
  "StartSensor": "True"
}
